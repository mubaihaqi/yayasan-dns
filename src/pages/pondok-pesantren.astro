---
import Layout from "../layouts/Layout.astro";
import ComingSoonSection from "../components/react-components/ComingSoonSection.jsx";
---

<Layout>
  <div class="h-[104px]"></div>
  <div class="bg-background text-primary bg-grid-texture">
    <main class="min-h-[calc(100vh-104px)] flex items-center justify-center">
      <div class="container mx-auto py-32 px-4 sm:px-6 lg:px-8">
        <ComingSoonSection client:visible />
      </div>
    </main>
  </div>
</Layout>
